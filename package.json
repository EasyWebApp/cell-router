{
  "name": "cell-router",
  "version": "1.0.0-beta",
  "description": "Decorator-based Router component framework, powered by WebCell",
  "keywords": [
    "decorator",
    "router",
    "web-component",
    "web-cell",
    "web-app",
    "spa"
  ],
  "author": "shiy2008@gmail.com",
  "license": "LGPL-3.0",
  "homepage": "https://web-cell.tk/cell-router/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyWebApp/cell-router.git"
  },
  "bugs": {
    "url": "https://github.com/EasyWebApp/cell-router/issues"
  },
  "module": "source/index.js",
  "browser": "dist/cell-router.min.js",
  "directories": {
    "lib": "source/",
    "test": "test/"
  },
  "scripts": {
    "lint": "eslint source/ test/*.js --fix",
    "pack": "amd-bundle source/index dist/cell-router -m",
    "pack-test": "cd test  &&  web-cell pack",
    "test": "npm run lint  &&  npm run pack  &&  npm run pack-test  &&  mocha --exit",
    "debug": "npm run pack  &&  npm run pack-test  &&  mocha --inspect-brk --no-timeouts",
    "build": "npm run lint  &&  npm run pack  &&  esdoc",
    "prepublishOnly": "npm test",
    "help": "esdoc  &&  web-server docs/ -o"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test  &&  git add ."
    }
  },
  "dependencies": {
    "dom-renderer": "^0.8.1",
    "web-cell": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "amd-bundle": "^1.7.7",
    "babel-eslint": "^10.0.1",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-external-webapi-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.15.1",
    "husky": "^1.3.1",
    "jsdom": "^14.0.0",
    "koapache": "^1.0.6",
    "mocha": "^6.0.2",
    "puppeteer-browser": "^0.3.8",
    "should": "^13.2.3",
    "web-cell-cli": "^1.0.0-alpha"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "decoratorsBeforeExport": true
        }
      ],
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
