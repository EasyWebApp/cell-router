{
    "index": "ReadMe.md",
    "source": "./source",
    "destination": "./docs",
    "plugins": [
        {
            "name": "esdoc-standard-plugin",
            "option": {
                "test": {
                    "source": "./test",
                    "interfaces": ["describe", "it"],
                    "includes": ["\\.test\\.js$"]
                }
            }
        },
        {
            "name": "esdoc-external-webapi-plugin",
            "option": {
                "enable": true
            }
        },
        {
            "name": "esdoc-ecmascript-proposal-plugin",
            "option": {
                "all": true
            }
        }
    ]
}
