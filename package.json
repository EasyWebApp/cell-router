{
  "name": "cell-router",
  "version": "0.7.1",
  "description": "Router component based on WebCell",
  "keywords": [
    "web-component",
    "router",
    "web-cell",
    "web-app"
  ],
  "author": "shiy2008@gmail.com",
  "license": "LGPL-3.0",
  "homepage": "https://easywebapp.github.io/cell-router/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyWebApp/cell-router.git"
  },
  "bugs": {
    "url": "https://github.com/EasyWebApp/cell-router/issues"
  },
  "main": "source/index.js",
  "browser": "dist/source.js",
  "directories": {
    "lib": "source/",
    "test": "test/"
  },
  "scripts": {
    "build-test": "cd test  &&  web-cell pack",
    "start": "npm run build-test  &&  web-cell preview",
    "lint": "eslint source/ test/*.js --fix",
    "test": "mocha --exit",
    "debug": "mocha --inspect-brk",
    "build": "npm run lint  &&  web-cell pack  &&  npm test  &&  esdoc",
    "prepublishOnly": "npm run build",
    "help": "web-server docs/ -o"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build  &&  git add ."
    }
  },
  "dependencies": {
    "web-cell": "^0.10.10"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "@webcomponents/webcomponentsjs": "^2.2.1",
    "babel-eslint": "^10.0.1",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-external-webapi-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.9.0",
    "husky": "^1.2.0",
    "koapache": "^1.0.4",
    "mocha": "^5.2.0",
    "puppeteer-browser": "^0.3.7",
    "should": "^13.2.3",
    "should-sinon": "0.0.6",
    "sinon": "^7.1.1",
    "web-cell-cli": "^0.7.5"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "decoratorsBeforeExport": true
        }
      ]
    ]
  }
}
