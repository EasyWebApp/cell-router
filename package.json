{
  "name": "cell-router",
  "version": "1.0.0",
  "description": "Decorator-based Router component framework, powered by WebCell",
  "keywords": [
    "decorator",
    "router",
    "web-component",
    "web-cell",
    "web-app",
    "spa"
  ],
  "author": "shiy2008@gmail.com",
  "license": "LGPL-3.0",
  "homepage": "https://web-cell.dev/cell-router/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyWebApp/cell-router.git"
  },
  "bugs": {
    "url": "https://github.com/EasyWebApp/cell-router/issues"
  },
  "module": "source/index.js",
  "browser": "dist/cell-router.min.js",
  "directories": {
    "lib": "source/",
    "test": "test/"
  },
  "scripts": {
    "lint": "lint-staged",
    "pack": "amd-bundle source/index dist/cell-router -m",
    "pack-test": "cd test  &&  web-cell pack",
    "test": "npm run pack  &&  npm run pack-test  &&  mocha --exit",
    "debug": "npm run pack  &&  npm run pack-test  &&  mocha --inspect-brk --no-timeouts",
    "build": "npm run pack  &&  esdoc",
    "prepublishOnly": "npm test",
    "help": "esdoc  &&  web-server docs/ -o"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "dom-renderer": "^1.0.3",
    "web-cell": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "amd-bundle": "^1.7.8",
    "babel-eslint": "^10.0.3",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-external-webapi-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.16.0",
    "husky": "^2.7.0",
    "jsdom": "^15.1.1",
    "koapache": "^1.0.6",
    "lint-staged": "^9.2.5",
    "mocha": "^6.2.0",
    "puppeteer-browser": "^0.3.8",
    "should": "^13.2.3",
    "web-cell-cli": "^1.0.0"
  }
}
