{
  "name": "cell-router",
  "version": "0.4.1",
  "description": "Router component based on WebCell",
  "keywords": [
    "web-component",
    "router",
    "web-cell",
    "web-app"
  ],
  "author": "shiy2008@gmail.com",
  "license": "LGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyWebApp/cell-router.git"
  },
  "bugs": {
    "url": "https://github.com/EasyWebApp/cell-router/issues"
  },
  "homepage": "https://easywebapp.github.io/cell-router/",
  "main": "source/index.js",
  "browser": "dist/source.js",
  "directories": {
    "lib": "source/",
    "test": "test/"
  },
  "scripts": {
    "lint": "eslint source/",
    "precommit": "npm run lint",
    "build": "web-cell pack  &&  esdoc",
    "prepublishOnly": "npm run build",
    "help": "web-server docs/ -o",
    "build-test": "cd test  &&  web-cell pack",
    "start": "npm run build-test  &&  web-cell preview",
    "check": "web-cell pack  &&  mocha",
    "test": "npm run check -- --exit",
    "debug": "npm run check -- --inspect-brk"
  },
  "dependencies": {
    "web-cell": "^0.9.4"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "esdoc": "^1.1.0",
    "esdoc-external-webapi-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.0.1",
    "husky": "^0.14.3",
    "koapache": "^0.2.3",
    "mocha": "^5.2.0",
    "puppeteer-browser": "^0.3.1",
    "should": "^13.2.1",
    "should-sinon": "0.0.6",
    "sinon": "^6.1.0",
    "web-cell-cli": "^0.4.1"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false
        }
      ]
    ]
  }
}
