{
    "compilerOptions": {
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "downlevelIteration": true,
        "useDefineForClassFields": true,
        "jsx": "react-jsx",
        "jsxImportSource": "dom-renderer",
        "lib": ["ES2022", "DOM", "DOM.Iterable"],
        "skipLibCheck": true,
        "target": "ES2017"
    },
    "include": ["source/*"],
    "typedocOptions": {
        "name": "Cell Router",
        "excludeExternals": true,
        "excludePrivate": true,
        "readme": "./ReadMe.md",
        "plugin": ["typedoc-plugin-mdn-links"]
    }
}
